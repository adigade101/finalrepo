pipeline { 
    environment { 
        registry = "adigade101/test-repo1" 
        registryCredential = 'dockerhub_id' 
        dockerImage = '' 
    }
    agent any 
    stages { 
        stage('Cloning our Git') { 
            steps { 
                git branch:'main', url: 'https://github.com/adigade101/test-repo1.git' 
            }
        } 
        
       
    }
}
